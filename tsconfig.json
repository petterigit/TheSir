{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "esnext",
        "lib": ["esnext"],
        "sourceMap": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "noImplicitAny": true
    },
    "include": ["src/**/*"],
    "exclude": [".eslintrc.js", "block-cookies-extension"]
}
